 @{
    Layout = "~/Views/Shared/_InstructorLayout.cshtml";
    ViewData["Title"] = "Instructor DashBoard";
}

<div class="sb2-2">
                <!--== breadcrumbs ==-->
                <div class="sb2-2-2">
                    <ul>
                        <li><a href="index-2.html"><i class="fa fa-home" aria-hidden="true"></i> Home</a>
                        </li>
                        <li class="active-bre"><a href="#"> Add Admin</a>
                        </li>
                        <li class="page-back"><a href="index-2.html"><i class="fa fa-backward" aria-hidden="true"></i> Back</a>
                        </li>
                    </ul>
                </div>

                <!--== User Details ==-->
                <div class="sb2-2-3">
                    <div class="row">
                        <div class="col-md-12">
						<div class="box-inn-sp admin-form">
                                <div class="inn-title">
                                    <h4>Update Module</h4>
                                   
                                </div>
                                <div class="tab-inn">
                                    <form>
                                        <div class="row">
                                            <div class="input-field col s6">
                                                <input type="text" value="" class="validate" required>
                                                <label class="">Name</label>
                                            </div>
                                            <div class="input-field col s6">
                                                <input type="text" value="" class="validate" required>
                                                <label class="">Description</label>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="input-field col s6">
                                                <input type="text" value="" class="validate" required>
                                                <label class="">Content</label>
                                            </div>
                                            <div class="input-field col s6">
                                                <select>
									  <option value="" disabled selected>Select Status</option>
									  <option value="1">Active</option>
									  <option value="2">De-Active</option>
									  <option value="3">Delete</option>
									</select>
                                                <label class="">Course</label>
                                            </div>
                                        </div>      
                                        
                                        <div class="row">
											<div class="file-field input-field col s6">
												<div class="btn admin-upload-btn">
													<span>Image 1</span>
													<input type="file">
												</div>
												<div class="file-path-wrapper">
													<input class="file-path validate" type="text" placeholder="Image 1">
												</div>
											</div>

                                            <div class="file-field input-field col s6">
												<div class="btn admin-upload-btn">
													<span>Image 2</span>
													<input type="file">
												</div>
												<div class="file-path-wrapper">
													<input class="file-path validate" type="text" placeholder="Image 2">
												</div>
											</div>
                                        </div>      

                                        <div class="row">
											<div class="file-field input-field col s6">
												<div class="btn admin-upload-btn">
													<span>Pdf Upload 1</span>
													<input type="file">
												</div>
												<div class="file-path-wrapper">
													<input class="file-path validate" type="text" placeholder="Pdf Upload 1">
												</div>
											</div>

                                            <div class="file-field input-field col s6">
												<div class="btn admin-upload-btn">
													<span>Pdf Upload 2</span>
													<input type="file">
												</div>
												<div class="file-path-wrapper">
													<input class="file-path validate" type="text" placeholder="Pdf Upload 2">
												</div>
											</div>
                                        </div>      

                                        <div class="row">
											<div class="file-field input-field col s6">
												<div class="btn admin-upload-btn">
													<span>Video Upload 1</span>
													<input type="file">
												</div>
												<div class="file-path-wrapper">
													<input class="file-path validate" type="text" placeholder="Video Upload 1">
												</div>
											</div>

                                            <div class="file-field input-field col s6">
												<div class="btn admin-upload-btn">
													<span>Video Upload 2</span>
													<input type="file">
												</div>
												<div class="file-path-wrapper">
													<input class="file-path validate" type="text" placeholder="Video Upload 2">
												</div>
											</div>
                                        </div>      

                                        <div class="row">
                                            <div class="input-field col s12">
                                                <i class="waves-effect waves-light btn-large waves-input-wrapper" style=""><input type="submit" class="waves-button-input"></i>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
